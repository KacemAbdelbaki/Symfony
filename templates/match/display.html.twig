<table border="1">
    <tr>
        <th>Description</th>
        <th>Date</th>
        <th>Actions</th>
    </tr>
    {% for i in list %}
        <tr>
            <td>{{ i.description }}</td>
            <td>{{ i.date }}</td>
            <td><a href="{{ path('add_match') }}">add</a> <a href="{{ path('remove_match',{"id":i.id}) }}">delete</a> <a href="{{ path('modify_match',{"id":i.id}) }}">edit</a></td>
        </tr>
    {% endfor %}
</table>
